<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>Welcome</title>
	<meta property="og:site_name" content="Veritas Nacoss">
    <link rel="icon" href="/logo.png" type="image" sizes="67x75">
    <meta property="og:title" content="Welcome To Nacoss Offical Site" />
    <meta property="og:description" content="Let Connect VERTIAS" />
    <meta property="og:image" itemprop="image" content="/logo.png">
    <meta property="og:type" content="website" />
	<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
</head>
<body>
    <% include include/Anav%>
<br><br>
<div class="container">

    <h2>Total Register User : <%= data.length %></h2>
    <table class="table table-hover table-dark">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Name</th>
            <th scope="col">Matric Number</th>
            <th scope="col">Email</th>
            <th scope="col">Password</th>
          </tr>
        </thead>
        <tbody>
            <% var num = 1%>
        <% data.forEach(function(data) { %>
          <tr>
            <th scope="row"><%= num %></th>
            <td><%= data.name.toUpperCase()%></td>
            <td><%= data.username%></td>
            <td><%= data.email%></td>
            <td><%= data.password%></td>
          </tr>
          <%num +=1%>
          <% }); %>
        </tbody>
      </table>
    </div>
</body>
<% include include/script%>
</html>
